{
  "name": "@crystal-ball/react-app-prototype",
  "version": "1.0.0",
  "description": "Prototypical React application",
  "keywords": [
    "prototype",
    "react"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/react-app-prototype.git"
  },
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "storybook": "start-storybook -p 6006 --ci",
    "build": "NODE_ENV=production webpack --mode=production",
    "build:stats": "NODE_ENV=production webpack --mode=production --json > stats.json",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty --ext .js,.ts .jest .storybook cypress src",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:acceptance": "docker-compose up --exit-code-from cypress --abort-on-container-exit",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write '*.{js,json,md}' '**/*.{js,json,md,mdx,scss}'"
  },
  "dependencies": {
    "@babel/runtime": "7.8.4",
    "@emotion/core": "10.0.27",
    "@hot-loader/react-dom": "16.11.0",
    "@mdx-js/react": "1.5.5",
    "@reduxjs/toolkit": "1.2.3",
    "classnames": "2.2.6",
    "componentry": "3.0.0-rc.4",
    "core-js": "3.6.4",
    "dux-routing": "0.3.0",
    "emotion-theming": "10.0.27",
    "feather-icons": "4.26.0",
    "nanoid": "2.1.11",
    "path-to-regexp": "6.1.0",
    "prism-react-renderer": "1.0.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hot-loader": "4.12.19",
    "react-redux": "7.1.3",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.3",
    "svg-symbol-sprite-loader": "4.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.8.3",
    "@crystal-ball/commitizen-base": "2.3.0",
    "@crystal-ball/prettier-base": "1.6.0",
    "@crystal-ball/webpack-base": "7.14.0",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@percy/cypress": "2.3.0",
    "@storybook/addon-actions": "5.3.12",
    "@storybook/addon-docs": "5.3.12",
    "@storybook/addon-knobs": "5.3.12",
    "@storybook/addon-links": "5.3.12",
    "@storybook/addons": "5.3.12",
    "@storybook/react": "5.3.12",
    "@storybook/theming": "5.3.12",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.4.0",
    "cypress": "4.0.1",
    "eslint-config-eloquence": "13.1.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "jest-emotion": "10.0.27",
    "typescript": "3.7.5"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "prettier": "@crystal-ball/prettier-base"
}
