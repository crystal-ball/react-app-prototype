{
  "name": "@crystal-ball/react-app-prototype",
  "version": "1.0.0",
  "description": "Prototypical React application",
  "keywords": [
    "prototype",
    "react"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/react-app-prototype.git"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "storybook": "start-storybook -p 6006 --ci",
    "build": "NODE_ENV=production webpack --mode=production",
    "build:stats": "NODE_ENV=production webpack --mode=production --json > stats.json",
    "build:storybook": "NODE_ENV=production build-storybook",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty .",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:acceptance": "docker-compose up --exit-code-from cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write . !CHANGELOG.md !coverage !public"
  },
  "dependencies": {
    "@babel/runtime": "7.10.5",
    "@hot-loader/react-dom": "16.13.0",
    "@mdx-js/react": "1.6.14",
    "@reduxjs/toolkit": "1.4.0",
    "classnames": "2.2.6",
    "componentry": "3.0.0",
    "core-js": "3.6.5",
    "dux-routing": "0.5.0",
    "feather-icons": "4.28.0",
    "nanoid": "3.1.10",
    "path-to-regexp": "6.1.0",
    "prism-react-renderer": "1.1.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.21",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "svg-symbol-sprite-loader": "5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.10.4",
    "@crystal-ball/babel-base": "1.11.0",
    "@crystal-ball/commitizen-base": "2.7.1",
    "@crystal-ball/prettier-base": "2.2.0",
    "@crystal-ball/webpack-base": "9.9.0",
    "@percy/cypress": "2.3.1",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@storybook/theming": "5.3.19",
    "@testing-library/cypress": "6.0.0",
    "@testing-library/jest-dom": "5.11.1",
    "@testing-library/react": "10.4.7",
    "@types/jest": "26.0.7",
    "cypress": "4.11.0",
    "eslint-config-eloquence": "14.8.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "node-sass-utils": "1.1.3",
    "typescript": "3.9.7"
  },
  "prettier": "@crystal-ball/prettier-base",
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "renovate": {
    "extends": [
      "github>crystal-ball/renovate-base:project"
    ]
  }
}
