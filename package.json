{
  "name": "@crystal-ball/react-app-prototype",
  "version": "1.0.0",
  "description": "Prototypical React application",
  "keywords": [
    "prototype",
    "react"
  ],
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/react-app-prototype.git"
  },
  "engines": {
    "node": "12"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack serve --mode=development",
    "start:instrument": "BABEL_ENV=instrument NODE_ENV=development webpack serve --mode=development",
    "start:docker": "NODE_ENV=development webpack serve --host=0.0.0.0 --mode=development",
    "start:storybook": "NODE_ENV=development start-storybook -p 6006",
    "build": "NODE_ENV=production webpack --mode=production",
    "build:stats": "NODE_ENV=production webpack --mode=production --json > stats.json",
    "build:storybook": "NODE_ENV=production build-storybook",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty .",
    "test:types": "tsc --noEmit",
    "test:unit": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:acceptance": "docker-compose up --exit-code-from cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write . !CHANGELOG.md !coverage_cypress !coverage_jest !public"
  },
  "dependencies": {
    "@babel/runtime": "7.12.5",
    "@mdx-js/react": "1.6.21",
    "@opentelemetry/api": "0.12.0",
    "@opentelemetry/context-zone": "0.12.0",
    "@opentelemetry/exporter-collector": "0.12.0",
    "@opentelemetry/plugin-document-load": "0.11.0",
    "@opentelemetry/plugin-fetch": "0.12.0",
    "@opentelemetry/resources": "0.12.0",
    "@opentelemetry/tracing": "0.12.0",
    "@opentelemetry/web": "0.12.0",
    "@reduxjs/toolkit": "1.4.0",
    "classnames": "2.2.6",
    "componentry": "3.0.0",
    "core-js": "3.7.0",
    "dux-routing": "0.5.0",
    "nanoid": "3.1.18",
    "numeral": "2.0.6",
    "path-to-regexp": "6.2.0",
    "prism-react-renderer": "1.1.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "svg-symbol-sprite-loader": "5.0.0"
  },
  "devDependencies": {
    "@crystal-ball/babel-base": "3.0.0",
    "@crystal-ball/commitizen-base": "2.9.0",
    "@crystal-ball/prettier-base": "2.6.0",
    "@crystal-ball/webpack-base": "10.0.0",
    "@cypress/code-coverage": "3.8.5",
    "@percy/cypress": "2.3.3",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@storybook/addon-essentials": "6.1.10",
    "@storybook/addon-links": "6.1.10",
    "@storybook/addons": "6.1.10",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.1.10",
    "@storybook/theming": "6.1.10",
    "@testing-library/cypress": "7.0.2",
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/react": "11.2.2",
    "@types/jest": "26.0.16",
    "@types/mdx-js__react": "1.5.3",
    "@types/numeral": "0.0.28",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.11",
    "babel-plugin-istanbul": "6.0.0",
    "cypress": "6.0.1",
    "eslint-config-eloquence": "17.0.0",
    "eslint-plugin-mdx": "1.8.2",
    "husky": "4.3.4",
    "jest": "26.6.3",
    "node-sass-utils": "1.1.3",
    "postcss-nested": "5.0.2",
    "react-is": "17.0.1",
    "react-refresh": "0.9.0",
    "tailwindcss": "2.0.1",
    "terser-webpack-plugin": "4.2.3",
    "typescript": "4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "renovate": {
    "extends": [
      "github>crystal-ball/renovate-base:project"
    ]
  }
}
